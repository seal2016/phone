<?xml version='1.0' encoding='utf-8'?>
<widget id="com.seal.sealmovil" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>SealMovil</name>
    <description>Aplicación móvil para consultar su consumo y pagos.</description>
